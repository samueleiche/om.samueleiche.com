{
	"name": "om",
	"version": "1.5.3",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"lint": "eslint . --fix",
		"prettier": "prettier . --write",
		"type-check": "vue-tsc",
		"prepare": "husky"
	},
	"lint-staged": {
		"src/**/*.{js,ts,tsx,vue,scss}": [
			"npm run prettier",
			"npm run lint"
		],
		"./*.{js,ts,json}": [
			"npm run prettier",
			"npm run lint"
		],
		"*.html": "npm run prettier"
	},
	"dependencies": {
		"vue": "^3.5.14"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.2",
		"@types/node": "^22.15.21",
		"@vitejs/plugin-vue": "^5.2.4",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.5.0",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "9.27.0",
		"eslint-plugin-vue": "^10.1.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.1",
		"npm-run-all2": "^8.0.3",
		"prettier": "^3.5.3",
		"sass": "^1.89.0",
		"typescript": "~5.8.3",
		"vite": "^6.3.5",
		"vite-plugin-ejs": "^1.7.0",
		"vite-plugin-mkcert": "^1.17.8",
		"vite-plugin-pwa": "^1.0.0",
		"vue-tsc": "^2.2.10"
	}
}
