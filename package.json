{
	"name": "om",
	"version": "1.5.2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"lint": "eslint . --fix",
		"type-check": "vue-tsc",
		"prepare": "husky"
	},
	"lint-staged": {
		"src/**/*.{js,ts,tsx,vue,scss}": [
			"prettier --write",
			"npm run lint"
		],
		"./*.{js,ts,json}": [
			"prettier --write",
			"npm run lint"
		],
		"*.html": "prettier --write"
	},
	"dependencies": {
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
		"@types/node": "^22.10.5",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.2.0",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "9.17.0",
		"eslint-plugin-vue": "^9.32.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.3.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.4.2",
		"sass": "^1.83.1",
		"typescript": "~5.7.2",
		"vite": "^6.0.7",
		"vite-plugin-ejs": "^1.7.0",
		"vite-plugin-mkcert": "^1.17.6",
		"vite-plugin-pwa": "^0.21.1",
		"vue-tsc": "https://pkg.pr.new/vuejs/language-tools/vue-tsc@3fb59af"
	}
}
